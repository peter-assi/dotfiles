[core]
        excludesfile = ~/.gitexclude
[alias]
        co = checkout
        stat = status
        st = status
        ci = commit
        df = !git diff | vim -
        lg = !git log | vim -
        history = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short

